<% if !user_signed_in? %>
  <div class="jumbotron">
    <h1>Something big</h1>
    <p><%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary btn-lg" %></p>
  </div>
<% end %>
  <div class="row">
    <% @posts.each do |post| %>
      <div class="col-sm-6 col-md-3">
          <h3><%= post.title  %> by <%= post.user.email %></h3>
          <h4>From <%= post.from.name  %> to <%= post.to.name %> on <%= post.when_date %></h4>
      </div>
    <% end %>
  </div>
